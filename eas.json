{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_NO_TELEMETRY": "1",
        "YARN_VERSION": "4.7.0"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "credentialsSource": "remote",
      "env": {
        "NODE_ENV": "production",
        "EXPO_NO_TELEMETRY": "1",
        "YARN_VERSION": "4.7.0"
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_NO_TELEMETRY": "1",
        "YARN_VERSION": "4.7.0"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
